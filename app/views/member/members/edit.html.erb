<h2>登録情報編集</h2>
<%= form_with url:member_path, model:@member,local:true do |f| %>
<table>
  <tr>
   <th>氏名</th>
   <td>
     <%= f.text_field:last_name %>
     <%= f.text_field:first_name %>
   </td>
  </tr>
  <tr>
    <th>カナ</th>
    <td>
      <%= f.text_field:last_name_kana %>
      <%= f.text_field:first_name_kana %>
    </td>
  </tr>
  <tr>
    <th>郵便番号</th>
    <td>
      <%= f.text_field:post_code %>
    </td>
  </tr>
  <tr>
    <th>住所</th>
    <td>
      <%= f.text_area:address %>
    </td>
  </tr>
  <tr>
    <th>電話番号</th>
    <td>
      <%= f.text_field:phone_number %>
    </td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td>
      <%= f.text_field:email %>
    </td>
  </tr>
</table>
　 <p><%= f.submit "編集内容を保存" %></p>
<% end %>
   <%= link_to "退会する", unsubscribe_members_path, class:'btn btn-danger'%>
